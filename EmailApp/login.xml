<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/carbon">
	<Button
		android:id="@+id/loginButton"
		android:background="@drawable/rounded_button"
		android:layout_width="273dp"
		android:layout_height="44dp"
		android:layout_marginStart="69dp"
		android:layout_marginLeft="69dp"
		android:layout_marginTop="508dp"
		android:layout_marginEnd="69dp"
		android:layout_marginRight="69dp"
		android:layout_marginBottom="123dp"
		android:text="Login"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<EditText
		android:id="@+id/username"
		android:src="@drawable/email1"
		android:layout_width="284dp"
		android:layout_height="37dp"
		android:layout_marginStart="66dp"
		android:layout_marginLeft="66dp"
		android:layout_marginTop="243dp"
		android:layout_marginEnd="102dp"
		android:layout_marginRight="132dp"
		android:layout_marginBottom="220dp"
		android:textColorHint="@android:color/black"
		android:hint="Email:"
		android:background="@drawable/rounded_edittext"
		android:ems="10"
    android:alpha=".9"
    android:paddingLeft="30dp"
		android:inputType="textEmailAddress"
		app:layout_constraintBottom_toTopOf="@+id/loginButton"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.253"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<EditText
		android:id="@+id/password"
		android:layout_width="284dp"
		android:layout_height="39dp"
		android:layout_marginStart="42dp"
		android:layout_marginLeft="36dp"
		android:layout_marginTop="27dp"
		android:layout_marginEnd="49dp"
		android:layout_marginRight="59dp"
		android:layout_marginBottom="151dp"
		android:textColorHint="@android:color/black"
		android:hint="Password:"
		android:background="@drawable/rounded_edittext"
		android:ems="10"
    android:alpha=".9"
    android:paddingLeft="30dp"
		android:inputType="textPassword"
		app:layout_constraintBottom_toTopOf="@+id/loginButton"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="1.0"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/username"
		app:layout_constraintVertical_bias="1.0" />

	<ImageView
		android:id="@+id/emailIcon"
		android:layout_width="158dp"
		android:layout_height="136dp"
		android:layout_marginStart="126dp"
		android:layout_marginLeft="126dp"
		android:layout_marginTop="51dp"
		android:layout_marginEnd="127dp"
		android:layout_marginRight="127dp"
		android:layout_marginBottom="60dp"
		app:layout_constraintBottom_toTopOf="@+id/username"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/email1" />

	<ImageView
		android:id="@+id/usernameLogo"
		android:layout_width="38dp"
		android:layout_height="38dp"
		android:layout_marginStart="52dp"
		android:layout_marginLeft="37dp"
		android:layout_marginTop="252dp"
		android:layout_marginBottom="225dp"
		app:layout_constraintBottom_toTopOf="@+id/loginButton"
		app:layout_constraintEnd_toStartOf="@+id/username"
		app:layout_constraintHorizontal_bias="0"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/email" />

	<ImageView
		android:id="@+id/passwordLogo"
		android:layout_width="20dp"
		android:layout_height="20dp"
		android:layout_marginStart="52dp"
		android:layout_marginLeft="49dp"
		android:layout_marginTop="25dp"
		android:layout_marginBottom="152dp"
		app:layout_constraintBottom_toTopOf="@+id/loginButton"
		app:layout_constraintEnd_toStartOf="@+id/username"
		app:layout_constraintHorizontal_bias="0"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/usernameLogo"
		app:srcCompat="@drawable/key" />

	<TextView
		android:id="@+id/loginText"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="177dp"
		android:layout_marginLeft="199dp"
		android:layout_marginTop="17dp"
		android:layout_marginEnd="199dp"
		android:layout_marginRight="199dp"
		android:layout_marginBottom="24dp"
		android:textColor="#ffb702"
		android:text="Login"
		android:fontFamily="sans-serif-medium"
		android:textSize="50px"
		app:layout_constraintBottom_toTopOf="@+id/username"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/emailIcon" />
</android.support.constraint.ConstraintLayout>
